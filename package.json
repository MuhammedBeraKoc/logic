{
  "name": "lgc",
  "version": "0.1.0",
  "description": "An awesome logic library for JS",
  "main": "dist/index.js",
  "module": "dist/main.js",
  "scripts": {
    "format": "prettier --check --write \"src\"",
    "build:clear": "del-cli --force dist",
    "build:transpile": "babel src -d dist",
    "build:compress": "uglifyjs dist/utils.js dist/index.js -o dist/index.min.js -c -m",
    "build": "npm run format && npm run build:clear && npm run build:transpile",
    "format:test": "prettier --check --write \"test\"",
    "test": "npm run format:test && jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MuhammedBeraKoc/logic.git"
  },
  "keywords": [
    "logic",
    "boolean",
    "chaining"
  ],
  "author": "Bera",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MuhammedBeraKoc/logic/issues"
  },
  "homepage": "https://github.com/MuhammedBeraKoc/logic#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "del-cli": "^3.0.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "uglify-js": "^3.12.5"
  }
}
